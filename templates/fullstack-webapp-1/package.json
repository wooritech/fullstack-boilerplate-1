{
  "name": "fullstack-webapp-1",
  "version": "1.0.0-alpah.1",
  "description": "A template for fullstack webaap",
  "private": true,
  "scripts": {
    "infra-local": "docker-compose -f infra/local/docker-compose.yml -p fullstack-webapp-1",
    "infra-local:up": "run-s \"infra-local -- up -d\"",
    "infra-local:down": "run-s \"infra-local -- down\"",
    "infra-local:delete": "run-s \"infra-local -- down -v\"",
    "infra-local:ps": "run-s \"infra-local -- ps\"",
    "db:migrate": "prisma migrate save --experimental && prisma migrate up --experimental",
    "generate:prisma": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "studio": "prisma studio --experimental",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^0.1.1",
    "@next/bundle-analyzer": "^9.5.3",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.6.5",
    "babel-plugin-import": "^1.13.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.0",
    "less": "^3.12.2",
    "less-vars-to-js": "^1.3.0",
    "next": "^9.5.3",
    "next-compose-plugins": "^2.2.0",
    "nextjs-progressbar": "0.0.6",
    "nexus": "^0.26.1",
    "nexus-plugin-prisma": "^0.19.0",
    "null-loader": "^4.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.8.3",
    "sass": "^1.26.11",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.48",
    "@types/yup": "^0.29.7",
    "@wooritech/eslint-config": "^1.0.5",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
